{
  "name": "hella-simple-eleventy-tailwind-starter",
  "description": "Hella Simple Eleventy + Tailwind Starter",
  "scripts": {
    "serve": "npx @11ty/eleventy --serve --quiet & npx @tailwindcss/cli -i ./src/styles.css -o ./_site/styles.css --watch",
    "build": "rm -rf _site && npx tailwindcss -i ./src/styles.css -o ./_site/tailwind.css --minify && ELEVENTY_ENV=production npx @11ty/eleventy"
  },
  "keywords": [
    "eleventy",
    "tailwindcss",
    "starter"
  ],
  "author": "Marcel Appelman",
  "license": "MIT",
  "type": "module",
  "browserslist": "defaults, not ie <= 11",
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@tailwindcss/cli": "^4.1.5",
    "gh-pages": "^6.3.0",
    "tailwindcss": "^4.1.5"
  },
  "dependencies": {
    "@11ty/eleventy-img": "^6.0.4"
  }
}
